@model Book

<tr>
    <td>@Model.Title</td>
    <td>@Model.AuthorFirstName @Model.AuthorMiddleInitial @Model.AuthorLastName</td>
    <td>@Model.PageNumber</td>
    <td>@Model.Publisher</td>
    <td>@Model.ISBN</td>
    <td>$@Model.Price</td>
    <td>@Model.Category</td>
    <td>@Model.Classification</td>
</tr>